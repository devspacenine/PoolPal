<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    
    <item android:state_checked="true"
        android:state_window_focused="false" android:state_enabled="true"
        android:drawable="@drawable/btn_toggle_on_normal" />
    <item android:state_checked="true"
        android:state_window_focused="false" android:state_enabled="false"
        android:drawable="@drawable/btn_toggle_on_disabled" />
    <item android:state_checked="true" android:state_pressed="true"
        android:drawable="@drawable/btn_toggle_on_pressed" />
    <item android:state_checked="true"
        android:state_focused="true" android:state_enabled="true"
        android:drawable="@drawable/btn_toggle_on_focused" />
    <item android:state_checked="true" android:state_enabled="true"
        android:drawable="@drawable/btn_toggle_on_normal" />
    <item android:state_checked="true" android:state_focused="true"
        android:drawable="@drawable/btn_toggle_on_disabled_focused" />
    <item android:state_checked="true"
         android:drawable="@drawable/btn_toggle_on_disabled" />

    <item android:state_window_focused="false" android:state_enabled="true"
        android:drawable="@drawable/btn_toggle_off_normal" />
    <item android:state_window_focused="false" android:state_enabled="false"
        android:drawable="@drawable/btn_toggle_off_disabled" />
    <item android:state_pressed="true"
        android:drawable="@drawable/btn_toggle_off_pressed" />
    <item android:state_focused="true" android:state_enabled="true"
        android:drawable="@drawable/btn_toggle_off_focused" />
    <item android:state_enabled="true"
        android:drawable="@drawable/btn_toggle_off_normal" />
    <item android:state_focused="true"
        android:drawable="@drawable/btn_toggle_off_disabled_focused" />
    <item
         android:drawable="@drawable/btn_toggle_off_disabled" />
    
</selector>